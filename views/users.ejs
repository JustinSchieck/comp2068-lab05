<% include partials/header.ejs %>
    <h1>Users</h1>
  <!--Users Table-->
    <table>
        <thead>
            <tr>
                <th>Username</th>
                <th>ID</th>
            </tr>
        </thead>
        <tbody>
            <% for (var i = 0; i < accounts.length; i++) { %>
                <tr>
                    <td>
                        <%= accounts[i]._id %>
                    </td>
                    <td>
                        <%= accounts[i].username %>
                    </td>
                </tr>
                <% } %>
        </tbody>
    </table>
    </body>

    </html>
